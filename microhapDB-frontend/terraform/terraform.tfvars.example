# Example Terraform variables file
# Copy this to terraform.tfvars and customize for your environment

# AWS Configuration
aws_region = "us-east-2"

# Project Configuration
project_name = "microhapdb"
environment  = "prod"

# Backend Configuration
backend_url = "http://18.117.136.223"

# Optional: Custom Domain (leave empty to use CloudFront domain)
# domain_name     = "microhapdb.yourdomain.com"
# certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/12345678-1234-1234-1234-123456789012"

# Additional Tags
common_tags = {
  Project     = "MicrohapDB"
  ManagedBy   = "Terraform"
  Repository  = "https://github.com/tylerslonecki/microhapDB"
  Owner       = "Tyler Slonecki"
} 